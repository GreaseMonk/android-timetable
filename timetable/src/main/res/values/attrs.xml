<resources>
    <declare-styleable name="TimeTable">
        <attr name="calTitlesColor" format="color"/>
        <attr name="calNowTitlesColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SpannableBar">
        <attr name="barText" format="string"/>
        <attr name="barColor" format="color"/>
        <attr name="barTextColor" format="color"/>
        <attr name="barTextSize" format="dimension"/>
        <attr name="barStart" format="integer"/>
        <attr name="barSpan" format="integer"/>
        <attr name="barColumns" format="integer"/>
    </declare-styleable>
